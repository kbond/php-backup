        "symfony/filesystem": "~3.4|~4.0|~5.0"
    },
    "require-dev": {
        "symfony/process": "~3.4|~4.0|~5.0",
        "symfony/console": "~3.4|~4.0|~5.0",
        "symfony/finder": "~3.4|~4.0|~5.0",
        "league/flysystem": "~1.0",
        "phpunit/phpunit": "^5.7.11"
